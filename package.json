		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "next",
		"autoprefixer": "^10.3.7",
		"chai": "^4.3.4",
		"cssnano": "^5.0.8",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"ethereum-waffle": "^3.4.0",
		"ethers": "^5.5.1",
		"hardhat": "^2.6.7",
		"postcss": "^8.3.9",
		"postcss-load-config": "^3.1.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.42.6",
		"svelte-preprocess": "^4.9.8",
		"tailwindcss": "^2.2.16"
	},
	"type": "module"
}
